<h3>
  Login Page !!!!!!!
</h3>

<%= form_for(resource, as: resource_name, url: session_path(resource_name), html: {role: 'form'}) do |f| %>
    <%= devise_error_messages! %>
    <div class="form-group">
      <%= f.label :user_name %>
      <br/>
      <%= f.text_field :user_name, :autofocus => true, class: 'form-control input-lg', placeholder: 'Login' %>
    </div>

    <div class="form-group">
      <%= f.label :password %>
      <br/>
      <%= f.password_field :password, class: 'form-control input-lg', placeholder: 'Password' %>
    </div>

    <% if devise_mapping.rememberable? -%>
        <div class="checkbox">
          <%= f.check_box :remember_me %>
          <%= f.label :remember_me %>
        </div>
    <% end -%>

    <div>
      <%= f.submit "Sign in", class: 'btn btn-primary btn-large' %>
    </div>
<% end %>

<div class="user_links">
  <%= render "devise/shared/links" %>
</div>