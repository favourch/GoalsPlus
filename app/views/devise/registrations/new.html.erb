<section class="alt extra-space top" style="background-color:rgba(0, 44, 22, 0.80);">
  <div class="container">
    <div class="row clearfix">
      <div class="col-md-12 column">
        <div class="jumbotron">
          <h2>Register:</h2>
        </div>
      </div>
    </div>
  </div>
</section>


<section id="matchSection" class="alt extra-space">
  <div class="container">
    <div class="row clearfix">
      <div class="col-md-1 column">
      </div>
      <div class="col-md-9 column">


        <div class="frontGame" style="display: box">
          <div class="panel panel-default">
            <div class="match_body">

              <%= form_for(resource, :as => resource_name, :url => registration_path(resource_name), html: {role: 'form'}) do |f| %>
    <%= devise_error_messages! %>

    <div class="form-group">
      <%= f.label :user_name %>
      <br/>
      <%= f.text_field :user_name, :autofocus => true, placeholder: "Username", class: "form-control input-lg" %>
    </div>

    <div class="form-group">
      <%= f.label :email %>
      <br/>
      <%= f.email_field :email, placeholder: "email@example.com", class: "form-control input-lg" %>
    </div>

    <div class="form-group">
      <%= f.label :password %>
      <br/>
      <%= f.password_field :password, placeholder: "Password", class: "form-control input-lg" %>
    </div>

    <div class="form-group">
      <%= f.label :password_confirmation %>
      <br/>
      <%= f.password_field :password_confirmation, placeholder: "Confirm Password", class: "form-control input-lg" %>
    </div>

    <div><%= f.submit "Sign up", class: "btn btn-primary btn-large" %></div>
<% end %>

<div class="user_links">
  <%= render "devise/shared/links" %>
</div>

            </div>
          </div>
        </div>

      </div>
      <div class="col-md-1 column"></div>
    </div>
  </div>
</section>